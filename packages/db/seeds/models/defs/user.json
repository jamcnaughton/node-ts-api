{
  "id": {
    "isUUID": 4,
    "primaryKey": true,
    "unique": true,
    "allowNull": false,
    "defaultValue": "{{Sequelize.UUIDV4}}",
    "type": "{{Sequelize.UUID}}"
  },
  "password": {
    "type": "{{Sequelize.STRING}}"
  },
  "passwordSalt": {
    "type": "{{Sequelize.STRING}}"
  },
  "firstName": {
    "type": "{{Sequelize.STRING}}"
  },
  "surname": {
    "type": "{{Sequelize.STRING}}"
  },
  "email": {
    "unique": true,
    "validate": {
      "isEmail": true,
      "notEmpty": true
    },
    "type": "{{Sequelize.STRING}}"
  },
  "preferredLanguageId": {
    "type": "{{Sequelize.STRING}}"
  }
}